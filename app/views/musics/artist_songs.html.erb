<h1><%= @music.content %>曲一覧</h1>
<ul>
<div class="musics-container">
  <div class="music-box">
    <% @songs.each do |t| %>
      <div class="music_review">
        <iframe width="560" height="315" src="https://www.youtube.com/embed/<%= t.find_youtube_url %>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        <p><%= t.title %></p>
        <%= link_to "詳細へ", song_path(t.id) %>
      </div>
     <% end %>
  </div>
</div>
</ul>
